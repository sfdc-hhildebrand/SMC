<#if action.emptyStateStack>
        context.emptyStateStack();
<#else>
    <@compress single_line=true>
    <#if action.static>
          ${action.name}(
    <#else>
          ctxt.${action.name}(
    </#if>
    <#list action.arguments as arg>
        ${arg}
        <#if arg_has_nextg>
            ,
        </#if>
    </#list>
            )   ;
    </@compress>
</#if>
